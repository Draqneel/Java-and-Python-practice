            <Font size="24.0" />
         </font>
      </TextField>
      <Button fx:id="helpPhoneBtn" layoutX="332.0" layoutY="190.0" mnemonicParsing="false" onAction="#helpPhoneBtnAction" prefHeight="40.0" prefWidth="65.0" text="?">

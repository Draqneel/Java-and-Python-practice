            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="cancelBtn" layoutX="871.0" layoutY="44.0" mnemonicParsing="false" onAction="#cancelBtnAction" prefHeight="41.0" prefWidth="65.0" text="&lt;">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="saveChangeBtn" layoutX="706.0" layoutY="485.0" mnemonicParsing="false" onAction="#saveChangeBtnAction" prefHeight="63.0" prefWidth="264.0" text="Внести изменения">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <ImageView fx:id="iconImg" fitHeight="150.0" fitWidth="200.0" layoutX="800.0" layoutY="115.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resources/logo.png" />
